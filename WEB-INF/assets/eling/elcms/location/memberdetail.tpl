<img class="member-img" src="api/attachment/personalphoto/{{this.personalInfo.pkPersonalInfo}}">
<div class="member-text">
	<div class="detail-text">
		<h4><i class="icon-user"></i>  {{this.personalInfo.name}}</h4>
		<h4><i class="icon-building"></i>  {{this.memberSigning.room.number}}</h4>
		<h4><i class="icon-phone"></i>  {{this.personalInfo.phone}}</h4>
		<h4><i class="icon-tablet"></i>  {{this.personalInfo.mobilePhone}}</h4>
		<h4><i class="icon-asterisk"></i>  {{this.status.value}}</h4>
		
	</div>
</div>